{
  "name": "balinkbayan-cms-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "clean": "rimraf .next/ node_modules/ && pnpm i",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write '**/*.ts{,x}'",
    "check:types": "tsc",
    "check:spell": "git diff --name-only | npx cspell --no-summary --no-progress --no-must-find-files --file-list stdin",
    "check:all": "concurrently \"pnpm lint\" \"pnpm check:types\" \"pnpm check:spell\"",
    "pre-commit": "concurrently \"lint-staged --allow-empty\" \"pnpm check:types\" \"pnpm check:spell\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-query": "^5.59.19",
    "axios": "^1.8.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "lucide-react": "^0.454.0",
    "next": "^14.2.28",
    "next-auth": "^4.24.11",
    "next-plausible": "^3.12.4",
    "react": "^18.3.1",
    "react-date-picker": "^11.0.0",
    "react-dom": "^18.3.1",
    "react-drag-drop-files": "^2.3.10",
    "react-headless-pagination": "^1.1.6",
    "react-hook-form": "^7.53.1",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.1",
    "sonner": "^2.0.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@faker-js/faker": "^9.2.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@tanstack/react-query-devtools": "^5.59.19",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "concurrently": "^9.0.1",
    "cspell": "^8.15.7",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.6.3"
  }
}
